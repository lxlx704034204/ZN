page {
  background: #eee;
}

.withdrawTitle {
  width: 100%;
  height: 500rpx;
  background: #fff;
  position: relative;
}

.withdrawTitle >image {
  width: 40%;
  height: 40vw;
  margin: 60rpx 30%;
  position: absolute;
}

.withdrawTitle >text {
  width: 100%;
  position: absolute;
  text-align: center;
}

.withdrawTitle >text:nth-child(2) {
  width: 20%;
  margin: 10rpx 0 0 76%;
  text-align: right;
  color: rgba(114, 185, 233, 0.863);
  font-size: 24rpx;
}

.withdrawTitle >text:nth-child(3) {
  color: #666;
  font-size: 24rpx;
  margin-top: 380rpx;
}

.withdrawTitle >text:nth-child(4) {
  color: #333;
  font-size: 32rpx;
  margin-top: 410rpx;
}

.withdrawTitle >text:nth-child(4) text {
  font-size: 64rpx;
}

/* 申请提现 */

.applyMoney {
  width: 100%;
  height: 300rpx;
  text-align: center;
  background: #fff;
  margin: 20rpx 0;
  border-top: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
}

.applyMoney >view:nth-child(1) {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #333;
}

.applyMoney >view:nth-child(2) {
  width: 100%;
  height: 100rpx;
  padding: 20rpx 0;
  border-top: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
}

.applyMoney >view:nth-child(2)>view {
  width: 48%;
  float: left;
  margin-top: 10rpx;
}

.applyMoney >view:nth-child(2)>view:nth-child(1) {
  border-right: 1rpx solid #ddd;
}

.applyMoney >view:nth-child(2)>view >view:nth-child(1) {
  color: #999;
  font-size: 24rpx;
}

.applyMoney >view:nth-child(2)>view >view:nth-child(2) {
  color: #333;
  font-size: 32rpx;
}

.applyMoney >view:nth-child(3) {
  width: 80%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  margin-left: 10%;
  margin-top: 20rpx;
  background: green;
  border-radius: 10rpx;
  color: #fff;
}

/* 导航菜单 */

.menuList {
  width: 100%;
  height: 60rpx;
  position: relative;
  background: #fff;
  border-bottom: 1rpx solid #ddd;
}

.menuList >image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  margin: 5rpx 0 0 20rpx;
}

.menuList >text {
  width: 40%;
  line-height: 60rpx;
  font-size: 32rpx;
  color: #333;
  position: absolute;
  margin: 0 0 0 12%;
}

.menuList >view {
  position: absolute;
  width: 7px;
  height: 7px;
  border-top: 1px solid #867f7a;
  border-right: 1px solid #867f7a;
  transform: rotate(45deg);
  margin:18rpx 0 0 90%;
}
