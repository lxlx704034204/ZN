/* pages/withdraw/withdrawRecord/withdrawRecord.wxss */
.withdrawRecordList{
  width:100%;
  height: 120rpx;
  position: relative;
  border-bottom: 1rpx solid #ddd;
}
.withdrawRecordList > text{
  position: absolute;
}
.withdrawRecordList > text:nth-child(1){
  margin:20rpx 0 0 20rpx;
  font-size: 24rpx;
  color:#333;
  font-weight: 600;
}
.withdrawRecordList > text:nth-child(2){
  margin:60rpx 0 0 20rpx;
  font-size: 18rpx;
  color:#aaa;
}
.withdrawRecordList > text:nth-child(3){
  width:40%;
  margin:20rpx 0 0 58%;
  font-size: 20rpx;
  text-align: right;
  color:#aaa;
}
.withdrawRecordList > text:nth-child(4){
  width:40%;
  margin:50rpx 0 0 58%;
  font-size: 28rpx;
  text-align: right;
  color:rgb(19, 145, 15);
  font-weight: 600;
}
.withdrawRecordList > text:nth-child(5){
  width:40%;
  margin:90rpx 0 0 58%;
  font-size: 20rpx;
  text-align: right;
  color:#aaa;
}
.withdrawRecordList > text:nth-child(5) text{
  color:#666;
}

/* 加载完成 */
.loadOver{
  width:100%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 18rpx;
  color:#999;
}
