/* pages/withdraw/withdrawAccount/withdrawAccount.wxss */
.withdrawAccount{
  width:100%;
  height: 500rpx;
  font-size: 24rpx;
  color:#333;
}
.withdrawAccount >view{
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
  border-bottom:1rpx solid #ddd;
  position: relative;
}
.withdrawAccount >view:nth-child(1){
  font-size: 20rpx;
  color:#999;
}
.withdrawAccount >view:nth-child(1) image{
  width: 30rpx;
  height:30rpx;
  vertical-align:middle;
}
.withdrawAccount >view >text:nth-child(1){
  color:#999;
  position: absolute;
}
.withdrawAccount >view >text:nth-child(2){
  color:#333;
  margin-left: 18%;
  position: absolute;
}