/*轮播图样式*/

.swiperImage {
  height: 500rpx;
  margin-top: 50rpx;
}

.swiperImage image {
  width: 90%;
  height: 100%;
  margin: 0rpx 5%;
  background: #fff;
}

/*商品展示标题*/

.goodsTitle {
  width: 100%;
  height: 50rpx;
  text-align: center;
  font-size: 28rpx;
  line-height: 50rpx;
  margin-top: 50rpx;
  color: rgba(0, 0, 0, 0.6);
}

/*商品展示*/

.goods {
  width: 90%;
  height: 850rpx;
  margin: 0rpx 5%;
  padding: 1%;
  background: #fff;
}

.goods > image:nth-child(1) {
  width: 100%;
  height: 400rpx;
}

.goods > video:nth-child(1) {
  width: 100%;
  height: 400rpx;
}

.goods .goods_title {
  position: relative;
  width: 100%;
  height: 100rpx;
  border-bottom: 1px solid #ddd;
}

.goods .goods_title> view:nth-child(1) {
  width: 75%;
  height: 80rpx;
  position: absolute;
  margin: 10rpx 0 0 20rpx;
  padding: 15rpx 0 0 20rpx;
  font-size: 32rpx;
  font-weight: bold;
  border-right: 1rpx solid #ddd;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.goods .goods_title> view:nth-child(2) {
  width: 20%;
  height: 80rpx;
  position: absolute;
  margin: 10rpx 0rpx;
  padding: 10rpx 0;
  margin-left: 80%;
  text-align: center;
}

.goods .goods_title> view:nth-child(2) >view:nth-child(1) {
  font-size: 32rpx;
  color: red;
}

.goods .goods_title> view:nth-child(2) >view:nth-child(2) {
  font-size: 24rpx;
  color: #909399;
}

.goods .time {
  width: 100%;
  height: 80rpx;
  position: relative;
}

.goods .time > view {
  position: absolute;
  color: #ffa500;
  font-size: 24rpx;
  width: 50%;
}

.goods .time > view:nth-child(1) {
  margin: 5rpx 0 0 5rpx;
  padding-left: 10rpx;
}

.goods .time > view:nth-child(2) {
  margin: 40rpx 0 0 5rpx;
  padding-left: 10rpx;
}

.goods .time > view:nth-child(3) {
  margin: 5rpx 0 0 50%;
  padding-right: 10rpx;
  text-align: right;
}

.goods .time > view:nth-child(3)>text {
  color: red;
}

.goods .time > view:nth-child(4) {
  margin: 40rpx 0 0 50%;
  padding-right: 10rpx;
  text-align: right;
}

.price >view {
  color: red;
  font-size: 64rpx;
  font-weight: bold;
}

.price >view >text {
  font-size: 24rpx;
}

.price >view >text:nth-child(2) {
  color: #aaa;
  text-decoration: line-through;
}

.goods_btn {
  width: 100%;
  height: 80rpx;
  position: relative;
  border-bottom: 1rpx solid #ddd;
}

.goods_btn>view {
  height: 50rpx;
  width: 30%;
  text-align: center;
  line-height: 50rpx;
  position: absolute;
  margin: 20rpx;
  padding: 0 20rpx;
  border-radius: 50rpx;
  color: #fff;
  font-size: 28rpx;
}

.goods_btn>view:nth-child(1) {
  background: green;
}

.goods_btn>view:nth-child(2) {
  margin-left: 68%;
  background: red;
}

.buyer {
  width: 100%;
  height: 60rpx;
  position: relative;
  padding: 0 10rpx 0 20rpx;
  margin-top: 10rpx;
  padding-top: 10rpx;
  border-top: 1px soild #909399;
}

.buyer >view:nth-child(1) {
  position: absolute;
}

.buyer >view:nth-child(1)> image {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin-left: -5rpx;
  vertical-align: middle;
}

.buyer >view:nth-child(1)>text {
  font-size: 24rpx;
  margin-left: 10rpx;
  line-height: 40rpx;
  color: rgba(90, 86, 86, 0.938);
}

.buyer .icon-you {
  position: absolute;
  font-size: 34rpx;
  color: #909399;
  margin-left: 90%;
  margin-top: 15rpx;
}

/****商品直播*****/

.goodsLive {
  height: 680rpx;
}

.goodsLive_btn {
  border: 0;
}

.goodsLive_btn view {
  background: rgba(6, 2, 8, 0.6);
  margin: -80rpx 0 0 460rpx;
  font-size: 24rpx;
  text-align: center;
}
/*生鲜商品*/
.goShop{
  height: 100rpx;
  width: 100rpx;
  position: fixed;
  bottom: 650rpx;
  background: rgba(255, 246, 115, 0.863);
  right: 30rpx;
  padding:10rpx;
  border-radius: 50%;
  font-size: 24rpx;
  color:#000;
  text-align: center;
  line-height: 40rpx;
}
/*返回顶部*/
.goTop{
  height: 100rpx;
  width: 100rpx;
  position: fixed;
  bottom: 150rpx;
  background: rgba(0,0,0,.8);
  right: 30rpx;
  padding:10rpx;
  border-radius: 50%;
  font-size: 28rpx;
  color:#fff;
  text-align: center;
  line-height: 40rpx;
}


