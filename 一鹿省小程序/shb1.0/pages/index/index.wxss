.logs button{
  width: 702rpx;
  height: 88rpx;
  background:#FD0D1A;
  border-radius:20rpx;
  margin-bottom: 32rpx;
  color: #FFFFFF;
}
.logimg{
  width: 368rpx;
  height: 584rpx;
  margin: 180rpx 0 132rpx;
}
.logs .donot{
  background-color: #F4C41F;
}
button::after{
  border: 0;
}
.desc {
  width: 80%;
  padding-left: 100rpx;
  height: 100rpx;
  margin-top: 20rpx;
}
.desc .li{
  width: 100%;
  height:60rpx;
  line-height: 18rpx;
}
.desc image{
  width: 18rpx;
  height: 18rpx;
  margin:0 10rpx 0 0;
  vertical-align: middle;
}
.desc .li view{
  width:90%;
  height: 18rpx;
  font-size: 12px;
  color: #C6C4C4;
}