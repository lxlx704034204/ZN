/*消息通知动画*/
.news {
  width:60%;
  height:50rpx;
  margin-top: 150px;
  margin-left:10%;
  border:1px solid wheat;
  color:#fff;
  background: #aaa;
  border-radius: 20rpx;
  position: fixed;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  transition:1.5s;
  z-index: 10;
}
.news > image{
  width:50rpx;
  height:50rpx;
  margin-top:0rpx;
  margin-right:10rpx;
  vertical-align:middle;
  border-radius: 50%;
}
/*公司简介*/
.intro {
  width:90%;
  margin:20rpx 5%;
  display: flex;
  align-items: center;
  height: 140rpx;
  margin-bottom: 20rpx;
  padding: 6rpx 30rpx 8rpx;
  background: #fff;
}
.intro >view {  
    display: flex;
    flex-direction: column;
    overflow: hidden;
}
.intro> view >text{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.intro >image{
    flex-shrink: 0;
    width: 80rpx;
    height: 80rpx;
    margin-right: 20rpx;
}
.intro >view > text:nth-child(1) {
    font-size: 34rpx;
    color: #000;
    line-height: 1.3;
}
.intro >view > text:nth-child(2) {
    font-size: 24rpx;
    color: #909399;
}
.intro >view:nth-child(2){
  flex: 0.6;
  border-right:1px solid #909399; 
}
.intro >view:nth-child(3){
  flex: 0.2;
  text-align: center;
}
.intro >view:nth-child(3)>text:nth-child(1){
  color: green;
}
.intro >view:nth-child(4){
  flex: 0.2;
  text-align: center;
}
.intro >view:nth-child(4)>text:nth-child(1){
  color: red;
}

