/*商品展示*/
.goods{
  width:90%;
  height:850rpx;
  margin:0rpx 5%;
  padding:1%;
  background: #fff;
}
.goods > image:nth-child(1){
  width:100%;
  height: 400rpx;
}
.goods > video:nth-child(1){
  width:100%;
  height: 400rpx;
}
.goods .goods_title{
  position: relative;
  width:100%;
  height:100rpx;
  border-bottom: 1px solid #ddd;
}
.goods .goods_title> view:nth-child(1){
  width:75%;
  height: 80rpx;
  position: absolute;
  margin:10rpx 0 0 20rpx;
  padding:15rpx 0 0 20rpx;
  font-size: 32rpx;
  font-weight:bold;
  
  border-right: 1px solid #ddd;
}
.goods .goods_title> view:nth-child(2){
  width:20%;
  height: 80rpx;
  position: absolute;
  margin:10rpx 0rpx;
  padding:10rpx 0;
  
  margin-left:80%;
  text-align: center;
}
.goods .goods_title> view:nth-child(2) >view:nth-child(1){
  font-size: 32rpx;
  color: red;
}
.goods .goods_title> view:nth-child(2) >view:nth-child(2){
  font-size: 24rpx;
  
  color: #909399;
}
.goods .time{
  width:100%;
  height: 80rpx;
  position: relative;
}
.goods .time > view{
  position: absolute;
  color: #FFA500;
  font-size:24rpx;
  width:50%;
  
}
.goods .time > view:nth-child(1){
  margin:5rpx 0 0 5rpx;
  padding-left:10rpx;
}
.goods .time > view:nth-child(2){
  margin:40rpx 0 0 5rpx;
  padding-left:10rpx;
}
.goods .time > view:nth-child(3){
  margin:5rpx 0 0 50%;
  padding-right:10rpx;
  text-align: right;
}
.goods .time > view:nth-child(3)>text{
  color: red;
}
.goods .time > view:nth-child(4){
  margin:40rpx 0 0 50%;
  padding-right:10rpx;
  text-align: right;
}
.price >view{
  color:red;
  font-size: 64rpx;
  font-weight:bold;
}
.price >view >text{
  font-size: 24rpx;
}
.price >view >text:nth-child(2){
  color: #aaa;
  text-decoration:line-through;
}
.goods_btn{
  width:100%;
  height:80rpx;
  position: relative;
  border-bottom:1px solid #ddd; 
}
.goods_btn>view{
  height:50rpx;
  line-height: 50rpx;
  position: absolute;
  margin:20rpx;
  padding:0 20rpx;
  border-radius: 50rpx;
 
  color:#fff;
  font-size: 32rpx
}
.goods_btn>view:nth-child(1){
   background:green;
}
.goods_btn>view:nth-child(2){
  margin-left: 60%;
  background:red;
}
.buyer{
  width:100%;
  height: 60rpx;
  position: relative;
  padding:0 10rpx 0 20rpx;
  margin-top:10rpx;
  padding-top:10rpx;
